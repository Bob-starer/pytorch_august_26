import torch

x = torch.tensor([[1., 2.],
                  [3., 4.]])
y = torch.tensor([[4, 9],
                  [3, 4]])

'''
print(x.mean())         
print(x.mean(dim=0))     #columns
print(x.mean(dim=1))     #rows
'''

print(torch.mean(x))
print(x.dtype)

print(torch.matmul(x.type(torch.float32),y.type(torch.float32)))
